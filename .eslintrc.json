{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "import/extensions": ["error", "ignorePackages"],
    "implicit-arrow-linebreak": "off",
    "no-param-reassign":"off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "radix": ["error", "as-needed"]
  }
}
